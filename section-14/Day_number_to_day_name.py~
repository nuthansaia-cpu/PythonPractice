day_no=int(input("Enter the day number:"))
if day_no==0:
    print("Monday")
elif day_no==1:
    print("Tuesday")
elif day_no==2:
    print("Thursday")
elif day_no==3:
    print("Wenesday")
elif day_no==4:
    print("Friday")
elif day_no==5:
    print("Saturday")
elif day_no==6:
    print("Sunday")
else:
    print("Invalid Day Number")
